<%= form_for @event do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :venue %>
  <%= f.text_field :venue %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :start_at, 'From' %>
  <%= f.text_field :start_at %>

  <%= f.label :end_at, 'To' %>
  <%= f.text_field :end_at %>

  <%= f.submit %>

  <%= render 'shared/errors', object: @event %>

<% end %>